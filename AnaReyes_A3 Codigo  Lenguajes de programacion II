            // TODO: esta línea de código carga datos en la tabla 'empresaUNIDataSet.Directivo' Puede moverla o quitarla según sea necesario.
            this.directivoTableAdapter.Fill(this.empresaUNIDataSet.Directivo);
            // TODO: esta línea de código carga datos en la tabla 'empresaUNIDataSet.CentroTrabajo' Puede moverla o quitarla según sea necesario.
            this.centroTrabajoTableAdapter.Fill(this.empresaUNIDataSet.CentroTrabajo);

        }

